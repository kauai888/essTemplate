# Frontend Environment Variables (.env.example)
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# App Configuration
REACT_APP_NAME=ESS - Employee Self-Service
REACT_APP_VERSION=1.2.0

# Features
REACT_APP_GEOLOCATION_ENABLED=true
REACT_APP_OTP_ENABLED=true
REACT_APP_ANNOUNCEMENTS_ENABLED=true

# Debug Mode
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info
